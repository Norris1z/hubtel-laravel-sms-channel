{
    "name": "norris1z/hubtel-laravel-sms-channel",
    "description": "Hubtel SMS Channel for Laravel 5.3+",
    "homepage": "https://github.com/norris1z/hubtel-laravel-sms-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Norris Oduro Tei",
            "email": "norrisjibril@gmail.com",
            "homepage": "https://norrisoduro.carrd.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Hubtel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Hubtel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Hubtel\\HubtelServiceProvider"
            ]
        }
    }
}
